.list-search-box{
  position: relative;
  height: 40px;
  overflow: hidden;
  margin: 15px 0;
}
.list-search-bg{
    background: #ebebeb;
}
.list-search-input{
  position: relative;
  display: inline-block;
  box-sizing: border-box;
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  text-align: center;
  color: #a19e9e;
 padding: 0 10px;
}
.search-box {
  position:absolute;
  width:96%;
  height:40px;
  line-height:40px;
  left:2%;
  right:2%;
  top:0;
}
.list-placeholder{
    font-size: 11pt;
    color: #a19e9e;
}
