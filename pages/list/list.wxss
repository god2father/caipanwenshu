.view-list-page{
  background: #f9f7f7;
}
.list-items{
  display: block;
  overflow: hidden;
  background: #fff;
  border-top: 1px solid #d6d6d6;
  padding: 15px 18px;
}
.list-item-title{
  font-size: 14pt;
  color: #000;
  padding-bottom: 10px;
}
.list-item-mid{
  font-size: 12pt;
  color: #888;
  margin-bottom: 10px;
}
.list-item-bottom{
   font-size: 12pt;
  color: #888;
}