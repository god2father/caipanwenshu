/**index.wxss**/
.header{
  position: relative;
  width: 100%;
  min-height: 187px;
}  
.head-img{
  width: 100%;
  height: auto;
}
.search-box{
  position: absolute;
  width: 96%;
  height: 40px;
  line-height: 40px;
  left: 2%;
  right: 2%;
  top:15px;
  background: rgba(255, 255, 255, .2);
}
.search-input{
  position: relative;
  display: inline-block;
  box-sizing: border-box;
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  text-align: left;
  color: #fff;
  padding-left: 45px;
  padding-right: 40px;
}
.search-input::before{
  position: absolute;
  content: '';
  left: 15px;
  top: 10px;
  width: 18px;
  height: 19px;
  background: url(data:image/png;base64,) no-repeat 100% 100%;
  background-size: cover;
}
.colorfff{
  color: #fff;
}
.menu-list{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  justify-content: space-between;
}
.menu-list-item{
  display: flex;
  width:33.33%;
  margin:20px 0 20px 0;
  text-align: center;
}
.menu-list-item navigator{
  display: inline-block;
  margin: 0 auto;
  width: 100%;
}
.menu-list-item navigator:hover{
  background: none;
}
.menu-list-item-img{
  width: 40%;
  margin-bottom: 10px;
}
.menu-list-item-title{
  /*display: block;*/
  font-size: 12pt;
  text-align: center;
}
.bottom-box{
  background: #f9f7f7;
  padding-top: 10px;
  display: flex;
  justify-content: space-between;
  font-size: 11pt;
  margin-bottom: 10px;
}
.bottom-bg{
  display: flex;
  justify-content: space-between;
  width: 100%;
  background: #fff;
  padding-top: 10px;
}
.bottom-left{
  display: flex;
  width: 50%;
  box-sizing: border-box;
  background: #fff;
  padding: 20px 0;
  align-items: center;
  border-right: 1px solid transparent;
  border-image:url(data:image/png;base64,) 1 1 stretch;
}
.bottom-left image{
  display: flex;
  width: 100%;
}
.bottom-left-item{
  width: 100%;
  display: flex;
}
.bottom-left1{
  width: 35%;
  margin: 0 5% 0 10%;
  /*padding-left: 10%;*/
}
.bottom-left1-r{
  width: 65%;
}
.bottom-small-title{
  font-size: 14pt;
}
.bottom-small-txt{
  color: #565656;
  font-size: 12pt;
}
.bottom-right{
  display: flex;
  flex-wrap: wrap;
  width: 50%;
  box-sizing: border-box;
  background: #fff;
  padding: 20px 5px;
 
}
.bottom-right view{
  display: flex;
 
}
.bottom-right view text{
  margin: 0 0 10px 0;
  font-size: 10px;
  color: #8e6150;
   overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}